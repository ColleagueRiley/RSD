namespace RSD{

struct linkedList{
   linkedList* next;
   linkedList* prev;
   void* data;
};
#ifdef RSDSTRING
struct string{
    // data functions

    int size(); // gets the size of the string
    void c_str(); // generates a char* and returns it

    // init functions
    string(){}
    string(std::string s);
    string(char* s);
    string(char c);
    string(int i);
    string(int* a);

    private:
        linkedList data;
                
};


